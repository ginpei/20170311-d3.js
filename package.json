  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "d3": "^4.7.2",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "exorcist": "^0.4.0",
    "watchify": "^3.9.0"
  }
}
